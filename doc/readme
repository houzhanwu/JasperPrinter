sys.properties
#0版本号
version=
#1 打印类型[xml,database] 默认xml
printtype=xml
#2 模式[main|slave|client] 默认main
mode=main
#3 模版位置
rptpath=
#4 等待队列位置，mode=main 或者mode=slave的时候 xml需要额外配置为FTP的服务端路径
xmlpath=
#5 工作区位置
stationpath=
#6 PDF 生成位置，用于清理PDF
pdfpath=
deletepdf=
###################################
###7-8项为主服务器配置项，9为主从服务器配置项###
#7 主服务器的本地jar文件路径,需要额外配置为FTP服务端路径
mainexe=
#8 主服务器的本地jasper文件路径，需要额外配置为FTP服务端路径
mainjasper=
#9 主服务器的本地分发队列，需要额外配置为FTP服务端路径
maindeliver=
####################################
### 10-12项为从，客服务器 的宗主服务器配置项###
#10 IP
server_ip=
server_port=21
#11 username
server_username=
#12 password
server_password=
##13-14项为宗主服务器为主服务器时的配置项##
#13 主服务器的EXE FTP相对路径
ftp_mainexe=
#14 主服务器的jasper FTP相对路径
ftp_mainjasper=
#15 宗主服务器的分发 FTP相对路径
ftp_serverdeliver=
#16 打印机注册列表，客户机注册自己打印机，主从服务器注册自己从属服务器的打印机汇总
print_list=
#############################################################
#17 数据库打印连接字符串，打印类型为database时配置，同时 4,5项不需要配置
url_path=
#############################################################
#18 邮件认证
mail.smtp.auth=true
#19 邮件服务器地址
mail.smtp.host
#20 邮件用户名
mail.user
#21 邮件密码
mail.password
----------------------------------------------------------
不要将station目录囊括在FTP工作目录中！！！！！！！！！！！！！！！
